@inherits ALifecycleComponent

<li>@Id + @Value</li>

@code {
    private static int Counter { get; set; } = 0;
    private int Id { get; set; }

    [Parameter]
    public string Value { get; set; }

    protected override void OnInitialized()
    {
        Id = ++Counter;
        base.OnInitialized();
    }
}
